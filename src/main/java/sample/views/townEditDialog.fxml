<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.TownEditDialogController">
   <children>
      <GridPane layoutX="77.0" layoutY="31.0" prefHeight="190.0" prefWidth="431.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Название города" />
            <Label text="Информация" GridPane.rowIndex="1" />
            <MediaView fitHeight="200.0" fitWidth="200.0" />
            <TextField fx:id="townEditName" GridPane.columnIndex="1" />
            <TextField fx:id="townEditInfo" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="151.0" layoutY="299.0" mnemonicParsing="false" onAction="#handleOk" text="Создать" />
      <Button layoutX="433.0" layoutY="299.0" mnemonicParsing="false" onAction="#handleCancel" text="Назад" />
   </children>
</AnchorPane>
